<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MemberDAO">

	<resultMap type="member" id="MemberDAO">
		<result property="code" column="member_code"/>
		<result property="id" column="member_id"/>
		<result property="password" column="member_password"/>
		<result property="phonenumber" column="member_phonenumber"/>
		<result property="email" column="member_email"/>
		<result property="zipcode" column="member_zipcode"/>
		<result property="address" column="member_address"/>
		<result property="detailaddress" column="member_detailaddress"/>
		<result property="image" column="member_image"/>
	</resultMap>

	<select id="selectLastest" resultType="member">
		SELECT * FROM MEMBER WHERE MEMBER_ID = (SELECT MAX(MEMBER_ID) FROM MEMBER);
	</select>
	
</mapper>
